<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:ui="using:FluentAvalonia.UI.Controls"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="USTMaker.Entry">
  <Grid ColumnDefinitions="3*,6*,1*,42">
    <TextBox x:Name="key" Grid.Column="0" Text="TextBox" HorizontalAlignment="Stretch"/>
    <TextBox x:Name="value" Grid.Column="1" Text="TextBox" HorizontalAlignment="Stretch"/>
    <Button x:Name="select" Grid.Column="2" Content="Select File" Classes="accent" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Click="select_Click" />
    <Button x:Name="delete" Grid.Column="3" Click="delete_Click" HorizontalAlignment="Stretch" VerticalAlignment="Stretch">
      <ui:FontIcon Glyph="&#xE106;" FontFamily="{StaticResource ResourceKey=SymbolThemeFontFamily}" />
    </Button>
  </Grid>
</UserControl>
